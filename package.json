{
  "name": "yarr",
  "version": "1.2.0",
  "license": "MIT",
  "author": "Eric Taylor <eric@daxos.com>",
  "description": "Yet another React Router.",
  "repository": "https://github.com/erictaylor/yarr",
  "main": "dist/index.js",
  "module": "dist/yarr.esm.js",
  "typings": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/yarr.esm.js",
    "default": "./dist/yarr.esm.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "react",
    "router",
    "concurrent",
    "route",
    "routing",
    "preloading",
    "prefetching",
    "relay",
    "suspense",
    "render-as-you-fetch"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint src",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "size-limit": [
    {
      "path": "dist/yarr.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/yarr.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@skypack/package-check": "^0.2.2",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-canonical": "^26.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-typescript-sort-keys": "^1.8.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "history": "^5.0.1"
  }
}
